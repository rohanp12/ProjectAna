{
  "name": "ana-conversation-studio",
  "version": "0.1.8",
  "main": "electron.js",
  "author": "Team Ana",
  "description": "The Ana app, contains Ana studio, user management, analytics and chatbot publishing.",
  "homepage": "http://ana.chat",
  "license": "GNU-GPLv3",
  "build": {
    "appId": "com.ana.studio",
    "productName": "Ana",
    "directories": {
      "output": "../release",
      "app": ".",
      "buildResources": "../build"
    },
    "mac": {
      "icon": "favicon.icns",
      "category": "public.app-category.graphics-design",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "favicon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "category": "Graphics",
      "icon": "favicon",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "electronVersion": "1.8.2",
    "publish": {
      "provider": "generic",
      "url": "https://cdn.ana.chat/dist/ana-app/${os}-${arch}"
    }
  },
  "dependencies": {
    "electron-updater": "^2.20.1",
    "keyword-extractor": "0.0.19",
    "spacy": "0.0.4",
    "spacy-nlp": "git+https://github.com/jeffreyflynt/spacy-nlp.git",
    "textrank": "^1.0.5"
  },
  "devDependencies": {
    "@angular/compiler": "^5.1.1",
    "@types/jquery": "^3.3.31"
  }
}
